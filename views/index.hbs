<h2>todo page</h2>

{{#if  todos.length}}
  <ul>
    {{#each todos}}
    <li>
        <label> 
          {{#if completed}}
            <input type="checkbox" checked>
            <span class="completed">{{title}}</span>
          {{else}}
            <input type="checkbox" checked>
            <span>{{title}}</span>
          {{/if}}
            <button>save</button>
        </label>
    </li>
    {{/each}}
  </ul>
{{else}}
<p>Not todos!</p>

{{/if}}